lastID <- id[1];
currentCumulDeltaMise = 0;
for(i in 2:nrow(DTAll)){
if(id[i] != lastID){
currentCumulDeltaMise = 0;
}else{
currentCumulDeltaMise = currentCumulDeltaMise + abs(mise[i] - mise[i-1]);
